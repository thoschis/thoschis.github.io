<script type="text/javascript">
	$(document).ready(function(){
	// toggles
		
		// directory
		$('#directory a').mouseenter(function(){$( '.left-pane' ).removeClass( 'off-left' );});
		$('#directory a').mouseleave(function(){$('.left-pane').addClass( 'off-left' );});
		$('#directory').mouseenter(function(){$('.bg-pane').addClass( 'bg-black' );});
		$('#directory').mouseleave(function(){$('.bg-pane').removeClass( 'bg-black' );});
	
		
		// toggle mobile menu
		//$( "#menu-toggle" ).click(function(){$( '.right-pane' ).removeClass( 'off-right-mobile' );});
		//$( "#menu-close" ).click(function(){$( '.right-pane' ).addClass( 'off-right-mobile' );});	
		
		// listing interactions
		$('#chessboards').mouseenter(function(){$( '.bg-pane' ).addClass( 'chessboards' );});
		$('#chessboards').mouseleave(function(){$('.bg-pane').removeClass( 'chessboards' );});
		$('#dustin-yellin-shreds-10k').mouseenter(function(){$( '.bg-pane' ).addClass( 'dustin-yellin-shreds-10k' );});
		$('#dustin-yellin-shreds-10k').mouseleave(function(){$('.bg-pane').removeClass( 'dustin-yellin-shreds-10k' );});	
		$('#elements').mouseenter(function(){$( '.bg-pane' ).addClass( 'elements' );});
		$('#elements').mouseleave(function(){$('.bg-pane').removeClass( 'elements' );});	
		$('#psychogeographies').mouseenter(function(){$( '.bg-pane' ).addClass( 'psychogeographies' );});
		$('#psychogeographies').mouseleave(function(){$('.bg-pane').removeClass( 'psychogeographies' );});
		$('#til-human-voices-wake-us').mouseenter(function(){$( '.bg-pane' ).addClass( 'til-human-voices-wake-us' );});
		$('#til-human-voices-wake-us').mouseleave(function(){$('.bg-pane').removeClass( 'til-human-voices-wake-us' );});
		$('#trees').mouseenter(function(){$( '.bg-pane' ).addClass( 'trees' );});
		$('#trees').mouseleave(function(){$('.bg-pane').removeClass( 'trees' );});		
		$('#caves').mouseenter(function(){$( '.bg-pane' ).addClass( 'caves' );});
		$('#caves').mouseleave(function(){$('.bg-pane').removeClass( 'caves' );});	
		$('#exhibit_2015-02_lincoln-center').mouseenter(function(){$( '.bg-pane' ).addClass( 'exhibit_2015-02_lincoln-center' );});
		$('#exhibit_2015-02_lincoln-center').mouseleave(function(){$('.bg-pane').removeClass( 'exhibit_2015-02_lincoln-center' );});	
