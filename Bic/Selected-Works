		$('#triptych').mouseleave(function(){$('.bg-pane').removeClass( 'triptych' );});
		$('#cities').mouseenter(function(){$( '.bg-pane' ).addClass( 'cities' );});
		$('#cities').mouseleave(function(){$('.bg-pane').removeClass( 'cities' );});	
		$('#crack-up').mouseenter(function(){$( '.bg-pane' ).addClass( 'crack-up' );});
		$('#crack-up').mouseleave(function(){$('.bg-pane').removeClass( 'crack-up' );});	
		$('#slides').mouseenter(function(){$( '.bg-pane' ).addClass( 'slides' );});
		$('#slides').mouseleave(function(){$('.bg-pane').removeClass( 'slides' );});
		$('#ant-farms').mouseenter(function(){$( '.bg-pane' ).addClass( 'ant-farms' );});
		$('#ant-farms').mouseleave(function(){$('.bg-pane').removeClass( 'ant-farms' );});
		$('#ethnographs').mouseenter(function(){$( '.bg-pane' ).addClass( 'ethnographs' );});
		$('#ethnographs').mouseleave(function(){$('.bg-pane').removeClass( 'ethnographs' );});					
		
		// listing interactions: text fill					
		//$('.text-entry').mouseenter(function(){$('.left-pane').addClass( 'bg-black' );});
		//$('.text-entry').mouseleave(function(){$('.left-pane').removeClass( 'bg-black' );});
		$('#scene-1').mouseenter(function(){$('#scene-1_text').addClass( 'text-fill-active' );});
		$('#scene-1').mouseleave(function(){$('#scene-1_text').removeClass( 'text-fill-active' );});
		$('#scene-2').mouseenter(function(){$('#scene-2_text').addClass( 'text-fill-active' );});
		$('#scene-2').mouseleave(function(){$('#scene-2_text').removeClass( 'text-fill-active' );});	
